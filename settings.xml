<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <mirrors>
        <mirror>
            <id>aliyun-proxy</id>
            <mirrorOf>*</mirrorOf>
            <name>Nexus</name>
            <url>http://nexus.middle-server:8081/repository/maven-public</url>
        </mirror>
    </mirrors>

    <repositories>
        <repository>
            <id>aliyun-proxy</id>
            <name>代理阿里云</name>
            <url>http://nexus.middle-server:8081/repository/maven-public</url>
            <layout>default</layout>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
    </repositories>
</settings>